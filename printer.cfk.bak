##加速度计
##########################################
# 5V # GND # PD0 # PD1 # PD3 # PD4 # PD5 #
##########################################

[mcu]
serial: /dev/serial/by-id/usb-Klipper_stm32f407xx_1E003A000947393436343431-if00


[printer]
kinematics: corexy
max_velocity: 450               
max_accel: 10000   		    	
max_accel_to_decel: 10000
max_z_velocity: 30              #15, max 15 for 12V TMC Drivers, can increase for 24V
max_z_accel: 350
square_corner_velocity: 5.0

[temperature_sensor FLY-Super8]
sensor_type: temperature_mcu


#####################################################################
# X/Y Stepper Settings 
# Z Stepper Settings   
#####################################################################
[include ./config/stepper.cfg]
[include ./config/tmc2209.cfg]



#
[extruder]
nozzle_diameter = 0.400
rotation_distance = 200 * 16 / 562
rotation_distance = 5.7 
microsteps = 16
filament_diameter = 1.750
step_pin: PE4
dir_pin: !PG0
enable_pin: !PG1
heater_pin: PB0
sensor_pin: PF4
sensor_type: ATC Semitec 104NT-4-R025H42G
control: pid
pid_Kp: 22.2
pid_Ki: 1.08
pid_Kd: 114
min_temp: -50
max_temp: 305
min_extrude_temp: 180

[heater_bed]
heater_pin: PE5
sensor_pin: PC1
sensor_type: EPCOS 100K B57560G104F
control: watermark
min_temp: -50
max_temp: 200

[fan]
pin: PA0 #FAN0
#cycle_time: 0.50

[controller_fan]
pin: PA1



[board_pins]
aliases:
    # EXP1 header
    EXP1_1=PE12, EXP1_3=PB2, EXP1_5=PC14, EXP1_7=PG14, EXP1_9=<GND>,
    EXP1_2=PE13, EXP1_4=PG8, EXP1_6=PC13, EXP1_8=PG13, EXP1_10=<5V>,
    # EXP2 header
    EXP2_1=PA6, EXP2_3=PB7, EXP2_5=PB6, EXP2_7=PG15,  EXP2_9=<GND>,
    EXP2_2=PA4, EXP2_4=PA4, EXP2_6=PA7, EXP2_8=<RST>, EXP2_10=<5V>

########################################
 #Mini 12864 LCD
########################################
[display]
lcd_type: uc1701
cs_pin: EXP1_3
a0_pin: EXP1_4
rst_pin: EXP1_5
contrast: 63
encoder_pins: ^EXP2_5, ^!EXP2_3
click_pin: ^!EXP1_2
spi_bus: spi1

